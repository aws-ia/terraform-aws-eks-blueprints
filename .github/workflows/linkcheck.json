{
    "timeout": "5s",
    "retryOn429": true,
    "retryCount": 5,
    "fallbackRetryDelay": "30s",
    "aliveStatusCodes": [200, 206],
    "httpHeaders": [
      {
        "urls": ["https://help.github.com/"],
        "headers": {
          "Accept-Encoding": "zstd, br, gzip, deflate"
        }
      }
    ],
    "ignorePatterns": [
      {
        "pattern": [
            "localhost"
        ]
      },
      {
        "pattern": [
            "127.0.0.1"
        ]
      }
    ]
}
